<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CreateProject">

    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="16dp"
        android:text="Create Projects"
        android:textAppearance="@style/TextAppearance.AppCompat.Display1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="248dp"
        android:layout_height="120dp"
        android:layout_marginTop="3dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Description"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/name" />




    <EditText
        android:id="@+id/name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="3dp"
        android:layout_marginEnd="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/editText3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title" />



    <com.google.android.material.chip.ChipGroup
        android:id="@+id/chipGroupLang"
        android:layout_width="386dp"
        android:layout_height="68dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LanguagesNeeded">

        <com.google.android.material.chip.Chip
            android:id="@+id/ruby"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="Ruby"
            app:layout_constraintBottom_toBottomOf="@+id/html"
            app:layout_constraintEnd_toStartOf="@+id/sql"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/html"
            app:layout_constraintTop_toTopOf="@+id/html" />

        <com.google.android.material.chip.Chip
            android:id="@+id/html"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="HTML"
            app:layout_constraintBottom_toBottomOf="@+id/javascript"
            app:layout_constraintEnd_toStartOf="@+id/ruby"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/javascript"
            app:layout_constraintTop_toTopOf="@+id/javascript" />

        <com.google.android.material.chip.Chip
            android:id="@+id/javascript"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="JavaScript"
            app:layout_constraintBottom_toBottomOf="@+id/java"
            app:layout_constraintEnd_toStartOf="@+id/html"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/java"
            app:layout_constraintTop_toTopOf="@+id/java" />

        <com.google.android.material.chip.Chip
            android:id="@+id/java"
            android:layout_width="67dp"
            android:layout_height="28dp"
            android:layout_marginTop="4dp"
            app:chipText="Java"
            app:layout_constraintEnd_toStartOf="@+id/javascript"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/css" />

        <com.google.android.material.chip.Chip
            android:id="@+id/cSharp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="C#"
            app:layout_constraintBottom_toBottomOf="@+id/python"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/python"
            app:layout_constraintTop_toTopOf="@+id/python" />

        <com.google.android.material.chip.Chip
            android:id="@+id/sql"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="SQL"
            app:layout_constraintBottom_toBottomOf="@+id/ruby"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/ruby"
            app:layout_constraintTop_toTopOf="@+id/ruby" />

        <com.google.android.material.chip.Chip
            android:id="@+id/dotNet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText=".NET"
            app:layout_constraintBottom_toBottomOf="@+id/cPlusPlus"
            app:layout_constraintEnd_toStartOf="@+id/python"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/cPlusPlus"
            app:layout_constraintTop_toTopOf="@+id/cPlusPlus" />

        <com.google.android.material.chip.Chip
            android:id="@+id/cPlusPlus"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="C++"
            app:layout_constraintBottom_toBottomOf="@+id/css"
            app:layout_constraintEnd_toStartOf="@+id/dotNet"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/css"
            app:layout_constraintTop_toTopOf="@+id/css" />


        <com.google.android.material.chip.Chip
            android:id="@+id/python"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="Python"
            app:layout_constraintBottom_toBottomOf="@+id/dotNet"
            app:layout_constraintEnd_toStartOf="@+id/cSharp"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/dotNet"
            app:layout_constraintTop_toTopOf="@+id/dotNet" />

        <com.google.android.material.chip.Chip
            android:id="@+id/css"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            app:chipText="CSS"
            app:layout_constraintEnd_toStartOf="@+id/cPlusPlus"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/LanguagesNeeded" />
    </com.google.android.material.chip.ChipGroup>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Databases"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chipGroupLang" />

    <com.google.android.material.chip.ChipGroup
        android:id="@+id/chipGroupDB"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5">

        <com.google.android.material.chip.Chip
            android:id="@+id/mongoDB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="MongoDB"
            app:layout_constraintBottom_toBottomOf="@+id/mysql"
            app:layout_constraintEnd_toStartOf="@+id/dynamoDB"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/mysql"
            app:layout_constraintTop_toTopOf="@+id/mysql" />

        <com.google.android.material.chip.Chip
            android:id="@+id/dynamoDB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="DynamoDB"
            app:layout_constraintBottom_toBottomOf="@+id/mongoDB"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/mongoDB"
            app:layout_constraintTop_toTopOf="@+id/mongoDB" />

        <com.google.android.material.chip.Chip
            android:id="@+id/mysql"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="MySQL"
            app:layout_constraintBottom_toBottomOf="@+id/postgresql"
            app:layout_constraintEnd_toStartOf="@+id/mongoDB"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/postgresql"
            app:layout_constraintTop_toTopOf="@+id/postgresql"
            app:layout_constraintVertical_bias="0.0" />

        <com.google.android.material.chip.Chip
            android:id="@+id/postgresql"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            app:chipText="Postgres"
            app:layout_constraintEnd_toStartOf="@+id/mysql"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

    </com.google.android.material.chip.ChipGroup>

    <TextView
        android:id="@+id/LanguagesNeeded"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Languages Needed"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText3" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Cloud Technology"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.469"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chipGroupDB" />

    <com.google.android.material.chip.ChipGroup
        android:id="@+id/chipGroupCloud"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6">


        <com.google.android.material.chip.Chip
            android:id="@+id/AWS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="AWS"
            app:layout_constraintBottom_toBottomOf="@+id/heroku"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/heroku"
            app:layout_constraintTop_toTopOf="@+id/heroku" />

        <com.google.android.material.chip.Chip
            android:id="@+id/heroku"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="Heroku"
            app:layout_constraintBottom_toBottomOf="@+id/firebase"
            app:layout_constraintEnd_toStartOf="@+id/AWS"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/firebase"
            app:layout_constraintTop_toTopOf="@+id/firebase" />

        <com.google.android.material.chip.Chip
            android:id="@+id/azure"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            app:chipText="Azure"
            app:layout_constraintEnd_toStartOf="@+id/firebase"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <com.google.android.material.chip.Chip
            android:id="@+id/firebase"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="Firebase"
            app:layout_constraintBottom_toBottomOf="@+id/azure"
            app:layout_constraintEnd_toStartOf="@+id/heroku"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/azure"
            app:layout_constraintTop_toTopOf="@+id/azure" />

    </com.google.android.material.chip.ChipGroup>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Platforms"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chipGroupCloud" />

    <com.google.android.material.chip.ChipGroup
        android:id="@+id/chipGroupPlatform"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7">


        <com.google.android.material.chip.Chip
            android:id="@+id/react"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="React"
            app:layout_constraintBottom_toBottomOf="@+id/linux"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/linux"
            app:layout_constraintTop_toTopOf="@+id/linux" />

        <com.google.android.material.chip.Chip
            android:id="@+id/web"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            app:chipText="Web"
            app:layout_constraintBottom_toBottomOf="@+id/android"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/android"
            app:layout_constraintTop_toTopOf="@+id/android"
            app:layout_constraintVertical_bias="1.0" />

        <com.google.android.material.chip.Chip
            android:id="@+id/iOS"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="iOS"
            app:layout_constraintEnd_toStartOf="@+id/android"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linux" />

        <com.google.android.material.chip.Chip
            android:id="@+id/android"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:chipText="Android"
            app:layout_constraintBottom_toBottomOf="@+id/iOS"
            app:layout_constraintEnd_toStartOf="@+id/web"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/iOS"
            app:layout_constraintTop_toTopOf="@+id/iOS"
            app:layout_constraintVertical_bias="0.0" />

        <com.google.android.material.chip.Chip
            android:id="@+id/linux"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            app:chipText="Linux"
            app:layout_constraintEnd_toStartOf="@+id/react"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <!--    end group-->
    </com.google.android.material.chip.ChipGroup>

    <EditText
        android:id="@+id/editText6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Link"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chipGroupPlatform" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Create"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText6" />
</androidx.constraintlayout.widget.ConstraintLayout>